        "df_code = pd.read_csv('https://goo.gl/tM6r3v', sep='\\t', dtype=str)\n",
        "df_code = df_code[df_code['폐지여부'] == '존재']\n",
        "df_code = df_code[['법정동코드', '법정동명']]\n",
        "df_code"
      ],
      "metadata": {
        "id": "BFsgkwJhB_Ln"
